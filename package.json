{
  "name": "ecom-monorepo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "start:storefront":"ng serve --project=ecom-storefront --port 4200",
    "start:admin":"ng serve --project=ecom-admin --port 4100",
    "start:delivery":"ng serve --project=ecom-delivery --port 4101",
    "start:inventory":"ng serve --project=ecom-inventory --port 4102",
    "start:order":"ng serve --project=ecom-order --port 4103",
    "start:supply":"ng serve --project=ecom-supply --port 4600",
    "start:user":"ng serve --project=ecom-user --port 4700",
    "start:payment":"ng serve --project=ecom-payment --port 4800",
    "start:price":"ng serve --project=ecom-price --port 4900",
    "start:product":"ng serve --project=ecom-product --port 4000",
    "prod:build:delivery":"ng build --prod --project ecom-delivery --base-href /delivery",
    "prod:build:inventory":"ng build --prod --project ecom-inventory --base-href /inventory",
    "prod:build:order":"ng build --prod --project ecom-order --base-href /order",
    "prod:build:supply":"ng build --prod --project ecom-supply --base-href /supply",
    "prod:build:user":"ng build --prod --project ecom-user --base-href /user",
    "prod:build:payment":"ng build --prod --project ecom-payment --base-href /payment",
    "prod:build:price":"ng build --prod --project ecom-price --base-href /price",
    "prod:build:product":"ng build --prod --project ecom-product --base-href /product",
    "prod:build:storefront":"ng build --prod --project ecom-storefront --base-href /ecom-storefront",
    "prod:build:admin":"ng build --prod --project ecom/admin --base-href /ecom-admin"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/cdk": "^12.2.13",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/material": "^12.2.13",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.17",
    "@angular/cli": "~12.2.12",
    "@angular/compiler-cli": "~12.2.0",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^12.1.1",
    "typescript": "~4.3.5"
  }
}
